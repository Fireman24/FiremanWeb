<UserControl x:Class="FiremanWpf.Controls.Filtered.FilteredListBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FiremanWpf.Controls.Filtered"
             mc:Ignorable="d" Height="19.8" Width="56"
             Name="this"
             >
    <Grid DataContext="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=UserControl}}">
        <TextBlock  HorizontalAlignment="Left" TextWrapping="Wrap" Text="{Binding NameStr}" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
